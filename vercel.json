{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "bun run build",
  "build": { "env": { "NODE_ENV": "production" } },
  "functions": {
    "dist/main.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [{ "source": "/(.*)", "destination": "/dist/main.js" }],
  "outputDirectory": "dist"
}
