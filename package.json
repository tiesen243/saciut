{
  "name": "saciut",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "dev": "cross-env NODE_ENV=development tsdown --watch",
    "format": "prettier --check . --cache --cache-location .cache/prettier",
    "format:fix": "prettier --check . --write --cache --cache-location .cache/prettier",
    "lint": "eslint . --cache --cache-location .cache/eslint",
    "lint:fix": "eslint . --fix --cache --cache-location .cache/eslint",
    "start": "cross-env NODE_ENV=production node --env-file-if-exists=.env dist/main.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "reflect-metadata": "^0.2.2",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "cross-env": "^10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
