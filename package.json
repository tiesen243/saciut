{
  "name": "saciut",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build && tsdown",
    "dev": "cross-env NODE_ENV=development tsdown --watch server",
    "format": "prettier --check . --cache --cache-location .cache/prettier",
    "format:fix": "prettier --check . --write --cache --cache-location .cache/prettier",
    "lint": "eslint . --cache --cache-location .cache/eslint",
    "lint:fix": "eslint . --fix --cache --cache-location .cache/eslint",
    "start": "cross-env NODE_ENV=production node --env-file-if-exists=.env dist/main.js",
    "typecheck": "react-router typegen && tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@react-router/express": "^7.8.2",
    "@react-router/node": "^7.8.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "isbot": "^5",
    "lucide-react": "^0.542.0",
    "morgan": "^1.10.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.2",
    "reflect-metadata": "^0.2.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@react-router/dev": "^7.8.2",
    "@tailwindcss/vite": "^4.1.12",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "cross-env": "^10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "tsdown": "^0.14.2",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
